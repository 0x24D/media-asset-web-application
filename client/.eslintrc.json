{
    "root": true,
    "env": {
        "browser": true,
        "es6": true,
        "node: true"
    },
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "extends": {
        "plugin:vue/essential",
        "@vue/airbnb"
    },
    "rules": {
        "no-console": process.env.NODE_ENV === "production" ? "error" : "off",
        "no-debugger": process.env.NODE_ENV === "production" ? "error" : "off",
        "import/prefer-default-export": "off",
        "no-console": "off",
        "no-underscore-dangle": ["error", { "allow": ["_id"] }]
    }
}
